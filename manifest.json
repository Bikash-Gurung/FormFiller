{
  "manifest_version": 3,
  "name": "Form Filler",
  "version": "0.0.1",
  "action": {
    "default_popup": "./popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./script/contentscript.js"],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "service_worker": "./script/serviceworker.js"
  },
  "permissions": ["tabs", "activeTab", "storage"]
}